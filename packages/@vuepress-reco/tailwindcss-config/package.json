{
  "name": "@vuepress-reco/tailwindcss-config",
  "version": "2.0.0-beta.30",
  "description": "Tailwindcss config for vuepress-theme-reco.",
  "keywords": [
    "vuepress-theme-reco",
    "tailwind",
    "vuepress"
  ],
  "author": "reco_luan <recoluan@qq.com>",
  "homepage": "https://vuepress-theme-reco.recoluan.com",
  "license": "MIT",
  "main": "lib/node/index.js",
  "types": "lib/node/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuepress-reco/vuepress-theme-reco.git"
  },
  "scripts": {
    "test": "echo \"Error: run tests from root\" && exit 1",
    "build": "tsc --build tsconfig.build.json",
    "build:watch": "tsc --build -w tsconfig.build.json",
    "clean": "rimraf lib *.tsbuildinfo",
    "copy": "cpx \"src/**/*.{d.ts,vue,scss,css,svg,js}\" lib"
  },
  "bugs": {
    "url": "https://github.com/vuepress-reco/vuepress-theme-reco/issues"
  }
}
